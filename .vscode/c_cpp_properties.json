{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/thirdparty/popstationmdg/src/plugins/*",
                "${workspaceFolder}/thirdparty/wxwidgets/include/wx/osx/*",
                "${workspaceFolder}/thirdparty/wxwidgets/include/*"
            ],
            "defines": [
                "__WXOSX_COCOA__",
                "__WXMAC__",
                "__WXOSX__"
            ],
            "macFrameworkPath": [],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Windows",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/thirdparty/popstationmdg/src/plugins/*",
                "${workspaceFolder}/thirdparty/wxwidgets/include/wx/msw/*",
                "${workspaceFolder}/thirdparty/wxwidgets/include/*"
            ],
            "defines": [
                "__WXMSW__",
                "_UNICODE",
                "_DEBUG",
                "BUILD_LIB"
            ],
            "compilerPath": "cl.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "msvc-x64"
        },
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/thirdparty/popstationmdg/src/plugins/*",
                "${workspaceFolder}/thirdparty/wxwidgets/include/wx/gtk/*",
                "${workspaceFolder}/thirdparty/wxwidgets/include/*"
            ],
            "defines": [
                "__WXGTK__"
            ],
            "compilerPath": "g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}